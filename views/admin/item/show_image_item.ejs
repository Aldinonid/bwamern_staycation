<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <!-- DataTables -->
  <div class="row">
    <% for( let x = 0; x < item.imageId.length; x++ ) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="https://bwamern-staycation.herokuapp.com/<%= item.imageId[x].imageUrl %>"
            alt="Image"
            style="width: 100%; display: block;"
          />
        </div>
      </div>
    </div>
    <% console.log(item.imageId[x].imageUrl); } %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
